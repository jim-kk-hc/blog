.delArticle + id, {\r\n                    withCredentials: true\r\n                }).then(\r\n                    res => {\r\n                        message.success('文章删除成功')\r\n                        getList()\r\n                    }\r\n                )\r\n            },\r\n            onCancel() {\r\n                message.success('没有任何改变')\r\n            },\r\n        });\r\n\r\n    }\r\n\r\n    //修改文章\r\n    const updateArticle = (id, checked)=>{\r\n\r\n        props.history.push('/index/add/'+id)\r\n\r\n    }\r\n    const [list, setList] = useState([])\r\n\r\n    useEffect(()=>{\r\n        getList()\r\n    },[])\r\n    return (\r\n        <div>\r\n             <List\r\n                header={\r\n                    <Row className=\"list-div\">\r\n                        <Col span={8}>\r\n                            <b>标题</b>\r\n                        </Col>\r\n                        <Col span={3}>\r\n                            <b>类别</b>\r\n                        </Col>\r\n                        <Col span={3}>\r\n                            <b>发布时间</b>\r\n                        </Col>\r\n                        <Col span={3}>\r\n                        </Col>\r\n                        <Col span={3}>\r\n                            <b>浏览量</b>\r\n                        </Col>\r\n\r\n                        <Col span={4}>\r\n                            <b>操作</b>\r\n                        </Col>\r\n                    </Row>\r\n\r\n                }\r\n                bordered\r\n                dataSource={list}\r\n                renderItem={item => (\r\n                    <List.Item>\r\n                        <Row className=\"list-div\">\r\n                            <Col span={8}>\r\n                                {item.title}\r\n                            </Col>\r\n                            <Col span={3}>\r\n                             {item.typeName}\r\n                            </Col>\r\n                            <Col span={3}>\r\n